{
  "extends": "@rushstack/heft-jest-plugin/includes/jest-shared.config.json",
  "coverageDirectory": "<rootDir>/coverage",
  "collectCoverage": true,
  "testMatch": ["<rootDir>/lib/**/*.test.cjs"],
  "collectCoverageFrom": [
    "lib/**/*.cjs",
    "!lib/**/*.d.ts",
    "!lib/**/*.test.cjs",
    "!lib/**/test/**",
    "!lib/**/__tests__/**",
    "!lib/**/__fixtures__/**",
    "!lib/**/__mocks__/**"
  ]
}
